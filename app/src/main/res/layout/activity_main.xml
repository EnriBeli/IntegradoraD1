<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00DDE3E7"
    tools:context=".MainActivity">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="16dp"
        app:cardCornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/signOutImageView"
        android:layout_width="49dp"
        android:layout_height="38dp"
        android:layout_gravity="end"
        android:layout_marginTop="16dp"
        android:background="@drawable/ic_buttom"
        android:contentDescription="@string/all"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.953"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_sign_out"
        tools:ignore="VectorDrawableCompat" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="336dp"
        android:layout_height="89dp"
        android:layout_marginStart="99dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="100dp"
        android:backgroundTint="#64C1EC"
        android:padding="0.2dp"
        app:cardBackgroundColor="@color/blue"
        app:cardCornerRadius="5dp"
        app:cardUseCompatPadding="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="336dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:orientation="vertical"
            android:padding="5dp">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/BienvenidoPrincipal"
                android:textColor="@color/white"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/NombresPrincipal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/NombresPrincipal"
                android:textColor="@color/white"
                android:textSize="20sp" />


        </androidx.appcompat.widget.LinearLayoutCompat>

    </androidx.cardview.widget.CardView>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="346dp"
        android:layout_height="125dp"
        android:layout_marginTop="230dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.369"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.12">


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/MiMedicoButton01"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="left"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/ic_buttom"
            android:drawableStart="@drawable/ice_user_medic"
            android:paddingLeft="5dp"
            android:paddingRight="20dp"
            android:text="@string/bottom_mimedico"
            android:textColor="@color/white"
            android:textSize="14dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/MisRecetasButton02"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="right"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="15dp"
            android:background="@drawable/ic_buttom"
            android:drawableStart="@drawable/baseline_archive_24"
            android:paddingLeft="5dp"
            android:paddingRight="20dp"
            android:text="@string/bottom_misrecetas"
            android:textColor="@color/white"
            android:textSize="14dp" />


    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="346dp"
        android:layout_height="125dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.369"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        app:layout_constraintVertical_bias="0.078">


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/MisMedicamentosButton03"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="left"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/ic_buttom"
            android:drawableStart="@drawable/ic_medication"
            android:paddingLeft="0dp"
            android:paddingRight="5dp"
            android:text="@string/bottom_mismedicamentos"
            android:textSize="14dp"
            android:textColor="@color/white" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/RegistrarCitaButton04"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="right"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="15dp"
            android:background="@drawable/ic_buttom"
            android:drawableStart="@drawable/baseline_edit_calendar_24"
            android:paddingLeft="2dp"
            android:paddingRight="20dp"
            android:text="Registrar cita"
            android:textSize="14dp"
            android:textColor="@color/white" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout14"
        android:layout_width="346dp"
        android:layout_height="125dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.369"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5"
        app:layout_constraintVertical_bias="0.178">


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/MisCitasButton05"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="left"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/ic_buttom"
            android:drawableStart="@drawable/baseline_list_24"
            android:paddingLeft="5dp"
            android:paddingRight="20dp"
            android:text="mis citas"
            android:textColor="@color/white"
            android:textSize="14dp" />


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/UbicacionButton06"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="right"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="15dp"
            android:background="@drawable/ic_buttom"
            android:drawableStart="@drawable/baseline_map_24"
            android:paddingLeft="5dp"
            android:paddingRight="20dp"
            android:text="Ubicacion"
            android:textColor="@color/white"
            android:textSize="14dp" />

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="261dp"
        android:layout_height="174dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/logo2"
        tools:layout_editor_absoluteY="1dp" />


</androidx.constraintlayout.widget.ConstraintLayout>